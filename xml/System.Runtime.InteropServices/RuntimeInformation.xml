      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Runtime.InteropServices.RuntimeInformation">
          <source>Provides information about the .NET runtime installation.</source>
          <target state="translated">Proporciona información sobre la instalación en tiempo de ejecución de .NET.</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="P:System.Runtime.InteropServices.RuntimeInformation.FrameworkDescription">
          <source>Returns a string that indicates the name of the .NET installation on which an app is running.</source>
          <target state="translated">Devuelve una cadena que indica el nombre de la instalación de .NET en la que se ejecuta una aplicación.</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Runtime.InteropServices.RuntimeInformation.FrameworkDescription">
          <source>The name of the .NET installation on which the app is running.</source>
          <target state="translated">Nombre de la instalación de .NET en el que se ejecuta la aplicación.</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Runtime.InteropServices.RuntimeInformation.FrameworkDescription">
          <source>The property returns one of the following strings:</source>
          <target state="translated">La propiedad devuelve una de las siguientes cadenas:</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Runtime.InteropServices.RuntimeInformation.FrameworkDescription">
          <source>".NET Core".</source>
          <target state="translated">".NET Core".</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Runtime.InteropServices.RuntimeInformation.FrameworkDescription">
          <source>".NET Framework".</source>
          <target state="translated">".NET Framework".</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Runtime.InteropServices.RuntimeInformation.FrameworkDescription">
          <source>".NET Native".</source>
          <target state="translated">".NET Native".</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="M:System.Runtime.InteropServices.RuntimeInformation.IsOSPlatform(System.Runtime.InteropServices.OSPlatform)">
          <source>A platform.</source>
          <target state="translated">Una plataforma.</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="M:System.Runtime.InteropServices.RuntimeInformation.IsOSPlatform(System.Runtime.InteropServices.OSPlatform)">
          <source>Indicates whether the current application is running on the specified platform.</source>
          <target state="translated">Indica si la aplicación actual se está ejecutando en la plataforma especificada o no.</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="M:System.Runtime.InteropServices.RuntimeInformation.IsOSPlatform(System.Runtime.InteropServices.OSPlatform)">
          <source><ph id="ph1">&lt;see langword="true" /&gt;</ph> if the current app is running on the specified platform; otherwise, <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</source>
          <target state="translated"><ph id="ph1">&lt;see langword="true" /&gt;</ph> si la aplicación actual se ejecuta en la plataforma especificada; en caso contrario, <ph id="ph2">&lt;see langword="false" /&gt;</ph>.</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="P:System.Runtime.InteropServices.RuntimeInformation.OSArchitecture">
          <source>Gets the platform architecture on which the current app is running.</source>
          <target state="translated">Obtiene la arquitectura de la plataforma en la que se ejecuta la aplicación actual.</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Runtime.InteropServices.RuntimeInformation.OSArchitecture">
          <source>The platform architecture on which the current app is running.</source>
          <target state="translated">Arquitectura de la plataforma en la que se ejecuta la aplicación actual.</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" uid="P:System.Runtime.InteropServices.RuntimeInformation.OSDescription">
          <source>Gets a string that describes the operating system on which the app is running.</source>
          <target state="translated">Obtiene una cadena que describe el sistema operativo en el que se ejecuta la aplicación.</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Runtime.InteropServices.RuntimeInformation.OSDescription">
          <source>The description of the operating system on which the app is running.</source>
          <target state="translated">La descripción del sistema operativo en el que se ejecuta la aplicación.</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve" uid="P:System.Runtime.InteropServices.RuntimeInformation.ProcessArchitecture">
          <source>Gets the process architecture of the currently running app.</source>
          <target state="translated">Obtiene la arquitectura de proceso de la aplicación que se está ejecutando.</target>       </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.Runtime.InteropServices.RuntimeInformation.ProcessArchitecture">
          <source>The process architecture of the currently running app.</source>
          <target state="translated">Arquitectura de proceso de la aplicación que se está ejecutando.</target>       </trans-unit>
